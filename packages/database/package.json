    "lint": "biome check ./src",
    "format": "biome format --write ./src"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^6.14.0",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.5.4"
  }
}
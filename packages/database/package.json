{
  "name": "@homejiak/database",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types-only.ts",
    "./postgres": "./src/postgres.ts"
  },
  "typesVersions": {
    "*": {
      "types": [
        "src/types-only.ts"
      ],
      "client": [
        "src/client.ts"
      ],
      "*": [
        "src/index.ts"
      ]
    }
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@homejiak/types": "workspace:*",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^6.14.0",
    "@upstash/redis": "^1.35.3",
    "bcryptjs": "^3.0.2",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.7",
    "prisma": "^6.14.0",
    "superjson": "^2.2.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.5.4"
  }
}
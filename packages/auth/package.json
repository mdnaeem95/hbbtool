{
  "name": "@kitchencloud/auth",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./client": {
      "import": "./src/client.ts",
      "require": "./src/client.ts"
    },
    "./server": {
      "import": "./src/server.ts",
      "require": "./src/server.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "require": "./src/utils/*.ts"
    },
    "./types": "./src/lib/database.types.ts"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "biome check ./src",
    "format": "biome format --write ./src"
  },
  "dependencies": {
    "@kitchencloud/database": "workspace:*",
    "@supabase/ssr": "^0.6.1",
    "dotenv": "^17.2.1",
    "nanoid": "^5.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "19.1.10",
    "react": "19.1.1",
    "supabase": "^2.34.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "next": "^14.2.0",
    "react": "18.3.1"
  }
}
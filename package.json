{
  "name": "kitchencloud",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "pnpm": {},
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:vercel": "cd packages/database && npx prisma generate && cd ../.. && turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "db:generate": "turbo run db:generate",
    "db:studio": "turbo run db:studio",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "turbo": "^2.0.11",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  }
}